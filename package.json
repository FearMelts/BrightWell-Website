  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.4.1",
    "postcss": "^8.5.2",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "medical-billing",
    "nextjs",
    "framer-motion",
    "healthcare",
    "typescript",
    "react",
    "tailwindcss"
