{
  "name": "brightwell-website",
  "version": "1.0.0",
  "private": true,
  "description": "Modern medical billing website built with Next.js 15, React 19, TypeScript, and Framer Motion",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
